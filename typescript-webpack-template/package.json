{
  "name": "typescript-webpack-template",
  "version": "0.0.1",
  "description": "A TypeScript project template built with Webpack 4.47.0",
  "main": "dist/index.js",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.js",
    "clean": "rimraf dist",
    "package": "cross-env NODE_ENV=production webpack --config build/webpack.prod.js",
    "build": "npm run package"
  },
  "keywords": [
    "typescript",
    "Webpack4",
    "npm"
  ],
  "author": "cole.yu",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.28.5",
    "@types/punycode": "^2.1.4",
    "babel-loader": "^8.3.0",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "less": "^4.2.0",
    "less-loader": "^7.3.0",
    "mini-css-extract-plugin": "^1.6.2",
    "minimatch": "^10.1.2",
    "postcss": "^8.4.31",
    "postcss-loader": "^4.3.0",
    "postcss-preset-env": "^7.8.3",
    "progress-bar-webpack-plugin": "^2.1.0",
    "rimraf": "^3.0.2",
    "url-loader": "^4.1.1",
    "vue": "2.7.15",
    "vue-loader": "^15.11.1",
    "vue-template-compiler": "2.7.15",
    "webpack": "^4.47.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "core-js": "^3.33.3",
    "ts-loader": "^4.5.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=12.18.4",
    "npm": ">=6.14.6",
    "ts-loader": "4.5.0",
    "typescript": "4.9.5"
  },
  "sideEffects": [
    "dist/*",
    "*.css",
    "*.less",
    "*.vue"
  ],
  "config": {
    "host": "0.0.0.0",
    "port": 8080
  }
}
